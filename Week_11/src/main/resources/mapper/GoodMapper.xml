<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.huwei.week11.dao.GoodDao">

    <!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap" type="com.huwei.week11.model.Good">
        <id column="good_id" property="goodId" />
        <result column="good_name" property="goodName" />
        <result column="good_stock" property="goodStock" />
    </resultMap>

    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        good_id, good_name, phone, good_stock
    </sql>

</mapper>
